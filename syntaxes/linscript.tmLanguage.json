{
  "scopeName": "source.linscript",
  "patterns": [
    {
      "match": "\\b0x[0-9A-Fa-f]+\\b",
      "name": "constant.numeric.hex.linscript"
    },
    {
      "match": "\"(?:[^\"\\\\]|\\\\.)*\"",
      "name": "string.quoted.double.linscript"
    },
    {
      "match": "\\b(Interaction|ScreenShake|Voice|Speaker|WaitFrame|WaitInput|Text|Sprite|Fade|Music|Sound|Background|LoadScript|ExecuteScript|Movie)\\b",
      "name": "entity.name.function.linscript"
    }
  ]
}
